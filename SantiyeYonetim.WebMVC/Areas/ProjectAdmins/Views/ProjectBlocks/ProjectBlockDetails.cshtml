@model SantiyeYonetim.WebMVC.Models.ProjectBlocks.ProjectBlockDetailViewModel

@{
    ViewData["Title"] = "ProjectBlockDetails";
    Layout = "~/Areas/ProjectAdmins/Views/Shared/_Layout.cshtml";
}

<h1>ProjectBlockDetails</h1>

<div class="container">

    @if (Model != null)
    {
        <div>
            <table class="table">
                <thead>
                    <tr>
                        <td>Id</td>
                        <td>Kat Adı</td>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var projectFloor in Model.ProjectBlock.ProjectFloors)
                    {
                        <tr>
                            <td>@projectFloor.Id</td>
                            <td>@projectFloor.Name</td>
                            <td><a href="/ProjectAdmins/ProjectFloors/ProjectFloorDetails/@projectFloor.Id" id="categoryListTableButton">Detay</a></td>
                            <td><a href="/ProjectAdmins/ProjectBlocks/Update/@projectFloor.Id" id="categoryListTableButton">Güncelle</a></td>
                            <td><a href="/ProjectAdmins/ProjectBlocks/Delete/@projectFloor.Id" id="categoryListTableButton">Sil</a></td>
                        </tr>
                    }
                </tbody>
            </table>

        </div>
    }
    else
    {
        <p>Hiç Kategori Bulunamadı</p>
    }
</div>